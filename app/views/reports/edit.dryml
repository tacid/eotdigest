<edit-page>
  <append-head:>
    <%= tinymce_assets %>
  </append-head:>
  <append-footer:>
    <%= tinymce %>
  </append-footer:>
  <form:>
    <field-list: fields="name, urlkey, content">
      <name-field: replace>
        <input/>
      </name-field:>
      <urlkey-field: replace>
        <label>URL ключ: <input style="width:30%;" placeholder="Путь по которому будет доступен отчет"/> <span class="help-inline">Оставьте пустым, чтобы закрыть публичный доступ.</span></label>
      </urlkey-field:>
      <content-field: replace>
        <input class="tinymce"/>
      </content-field:>
    </field-list:>
  </form>
</edit-page>
